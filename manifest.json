{
    "name": "Productivity",
    "description": "Enhance your efficiency by managing study or work sessions and stay motivated!",
    "version": "1.0.0",
    "manifest_version": 3,
    "author": "Jennier Yang",
    "permissions": ["storage", "activeTab", "tabs"],
    "background": {
        "service_worker": "background.js"
    },
    "action":{
        "default_icon": "images/blue_cat_icon.png"
    },
    "icons": {
        "16": "images/blue_cat_icon.png",
        "48": "images/blue_cat_icon.png",
        "128": "images/blue_cat_icon.png"
    }
}